    "jquery-mousewheel": "^3.1.13",
    "prevent-ghost-click": "0.0.0",
    "screenfull": "^2.0.0"
  },
  "devDependencies": {
    "bootstrap": "^3.3.5",
    "cz-conventional-changelog": "^1.2.0",
    "eslint": "^3.0.1",
    "eslint-config-standard": "^6.0.0-beta.0",
